{
  "name": "electron-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "chromedriver": "^92.0.1",
    "concurrently": "^6.2.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nodemon": "^2.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "selenium-webdriver": "^4.0.0-beta.4",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.0.1"
  },
  "main": "main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "e": "electron .",
    "electron-react": "concurrently --kill-others \"nodemon api/app.js\" \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "server-start": "concurrently --kill-others \"nodemon api/app.js\" \"BROWSER=none npm start\" \"wait-on http://localhost:3000\"",
    "e-build": "electron-builder",
    "web-start": "concurrently --kill-others \"nodemon api/app.js\" \"npm run start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^13.2.1",
    "electron-builder": "^22.11.7"
  }
}
